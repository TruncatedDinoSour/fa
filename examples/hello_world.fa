include 'std/std.fa'

"hello world\n" ro  -- Read only string
%STDOUT             -- Set file descriptor to stdout
%SYS_write sys 4    -- Call SYS_write
1 drop              -- Drop return value

%EXIT_SUCCESS       -- Set exit code to success
%SYS_exit sys 2     -- Call SYS_exit
1 drop              -- Drop return value
